          }
        }else {
          detailHtml+=`<div class="parameter-item">
            <h2 class="title">${textList[key]}</h2>
            <ul class="parameter-list">`;
          for (let i = 0; i < productDetail[key].length; i++) {
            const item = productDetail[key][i];
            if (item.val!==""){
              detailHtml+=`<li><span class="param-key">${item.key}：</span>
              <span class="param-val">${item.val}</span></li>`
            }
          }
          detailHtml+=`</ul></div>`
        }
      }
      $(".technology-head .name").text(_id);
      $(".technology-list").html(technologyHtml);
      $(".parameter-table").html(detailHtml);
    }
  });
</script>
